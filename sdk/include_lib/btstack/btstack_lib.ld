        BTSTACK_LE_HOST_MESH_BSS_SIZE = ABSOLUTE(. - BTSTACK_LE_HOST_MESH_BSS_START);

	/*Le audio big*/
        BTSTACK_LE_AUDIO_BIG_BSS_START = .;
        *(.big_rx_bss)
        *(.big_tx_bss)
        *(.big_common_bss)
