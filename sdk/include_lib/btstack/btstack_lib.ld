        sdp_record_item_begin = .;
        KEEP(*(.sdp_record_item))
        sdp_record_item_end = .;

        bt_sleep_begin = .;
        KEEP(*(.bt_sleep))
        bt_sleep_end = .;

	*(.bt_stack_const)
	*(.bt_stack_code)
        *(.ble_db_const)
        *(.ble_db_code)
        *(.ble_sm_const)
        *(.ble_sm_code)
        *(.ble_att_const)
        *(.ble_att_code)
